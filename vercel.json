{
  "functions": {
    "api/terms/[languageCode].js": {
      "maxDuration": 10
    },
    "api/config.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/config",
      "destination": "/api/config"
    },
    {
      "source": "/api/v1/locals/terms/(.*)",
      "destination": "/api/terms/$1"
    }
  ]
}
